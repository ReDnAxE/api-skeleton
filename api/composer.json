{
    "name": "rednaxe/api-skeleton",
    "type": "project",
    "license": "MIT",
    "description": "A minimal APi project recommended to create bare bones api",
    "authors": [
        {
            "name": "Alexandre GESLIN",
            "homepage": "https://github.com/rednaxe/api-skeleton"
        }
    ],
    "keywords": [
        "php",
        "behat"
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.0.0",
        "ext-ctype": "*",
        "ext-iconv": "*"
    },
    "require-dev": {
        "behat/behat": "3.8.x-dev"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "bin-dir": "bin/"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
